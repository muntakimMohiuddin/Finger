<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.IdentifyController">
    <children>
        <Label fx:id="firstNameText" alignment="CENTER" layoutX="124.0" layoutY="75.0" prefHeight="50.0" prefWidth="195.0" />
        <Label fx:id="lastNameText" alignment="CENTER" layoutX="124.0" layoutY="148.0" prefHeight="50.0" prefWidth="195.0" />
        <Label fx:id="nationalIdText" alignment="CENTER" layoutX="124.0" layoutY="213.0" prefHeight="50.0" prefWidth="195.0" />
        <Label fx:id="phoneNumberText" alignment="CENTER" layoutX="124.0" layoutY="286.0" prefHeight="50.0" prefWidth="195.0" />


        <ImageView id="fingerprintImageView" fx:id="fingerprintImageView" fitHeight="227.0" fitWidth="200.0" layoutX="386.0" layoutY="75.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@fingerprint_bg.png" />
            </image>
        </ImageView>
        <Label fx:id="fingerprintCapturedLabel" alignment="CENTER" layoutX="386.0" layoutY="311.0" prefHeight="27.0" prefWidth="170.0" textFill="#02b223">
            <font>
                <Font size="11.0" />
            </font>
        </Label>
        <ImageView id="backButton" fx:id="backButton" fitHeight="41.0" fitWidth="48.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#backButtonAction" pickOnBounds="true" preserveRatio="true" styleClass="fingerprint-image-view-wrapper glyph-icon">
            <image>
                <Image url="@back_button.png" />
            </image>
        </ImageView>
        <Label fx:id="firstNameText1" alignment="CENTER" layoutX="35.0" layoutY="75.0" prefHeight="50.0" prefWidth="86.0" text="First Name" />
        <Label fx:id="firstNameText11" alignment="CENTER" layoutX="35.0" layoutY="148.0" prefHeight="50.0" prefWidth="86.0" text="Last Name" />
        <Label fx:id="firstNameText12" alignment="CENTER" layoutX="35.0" layoutY="213.0" prefHeight="50.0" prefWidth="86.0" text="National ID" />
        <Label fx:id="firstNameText13" alignment="CENTER" layoutX="35.0" layoutY="286.0" prefHeight="50.0" prefWidth="86.0" text="Phone" />
        <Label fx:id="firstNameText14" alignment="CENTER" layoutX="55.0" layoutY="14.0" prefHeight="50.0" prefWidth="86.0" text="USER ID" />
        <Label fx:id="userIDText" alignment="CENTER" layoutX="159.0" layoutY="14.0" prefHeight="50.0" prefWidth="376.0" />
    </children>

</AnchorPane>
